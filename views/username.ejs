<h1><%= title %></h1>

<h2>hello, <%= user.username %></h2>

<form method="POST" action="<%= `/user/${user.username}` %>">
  <div>
    <input
      id="secretWord"
      placeholder="secret word"
      name="secretWord"
      type="text"
      required
    />
  </div>

  <% if (errors) for (error of errors) { %>
  <ul>
    <li><%= error.msg %></li>
  </ul>
  <% } %>
  <button type="submit">Submit</button>
</form>
